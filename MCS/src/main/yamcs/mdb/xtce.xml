					<ParameterRefEntry parameterRef="CDH_temperature"/>
					<ParameterRefEntry parameterRef="CDH_heater_setpoint"/>
					<ParameterRefEntry parameterRef="CDH_power_draw"/>	
					<ParameterRefEntry parameterRef="POWER_state"/>
					<ParameterRefEntry parameterRef="POWER_temperature"/>
					<ParameterRefEntry parameterRef="POWER_heater_setpoint"/>
					<ParameterRefEntry parameterRef="POWER_power_draw"/>
					<ParameterRefEntry parameterRef="POWER_battery_voltage"/>
					<ParameterRefEntry parameterRef="POWER_battery_current"/>
					<ParameterRefEntry parameterRef="POWER_battery_charge"/>
					<ParameterRefEntry parameterRef="POWER_total_power_balance"/>
					<ParameterRefEntry parameterRef="POWER_total_power_draw"/>
					<ParameterRefEntry parameterRef="POWER_total_power_generation"/>
					<ParameterRefEntry parameterRef="POWER_solar_panel_generation_pX"/>
					<ParameterRefEntry parameterRef="POWER_solar_panel_generation_nX"/>
					<ParameterRefEntry parameterRef="POWER_solar_panel_generation_pY"/>
					<ParameterRefEntry parameterRef="POWER_solar_panel_generation_nY"/>
					<ParameterRefEntry parameterRef="ADCS_state"/>
					<ParameterRefEntry parameterRef="ADCS_temperature"/>
					<ParameterRefEntry parameterRef="ADCS_heater_setpoint"/>
					<ParameterRefEntry parameterRef="ADCS_power_draw"/>
					<ParameterRefEntry parameterRef="ADCS_mode"/>
					<ParameterRefEntry parameterRef="ADCS_status"/>
					<ParameterRefEntry parameterRef="ADCS_quaternion_q1"/>
					<ParameterRefEntry parameterRef="ADCS_quaternion_q2"/>
					<ParameterRefEntry parameterRef="ADCS_quaternion_q3"/>
					<ParameterRefEntry parameterRef="ADCS_quaternion_q4"/>
					<ParameterRefEntry parameterRef="ADCS_angular_velocity_x"/>
					<ParameterRefEntry parameterRef="ADCS_angular_velocity_y"/>
					<ParameterRefEntry parameterRef="ADCS_angular_velocity_z"/>
					<ParameterRefEntry parameterRef="ADCS_earth_latitude"/>
					<ParameterRefEntry parameterRef="ADCS_earth_longitude"/>
					<ParameterRefEntry parameterRef="ADCS_earth_altitude"/>
					<ParameterRefEntry parameterRef="ADCS_eclipse"/>
					<ParameterRefEntry parameterRef="COMMS_state"/>
					<ParameterRefEntry parameterRef="COMMS_temperature"/>
					<ParameterRefEntry parameterRef="COMMS_heater_setpoint"/>
					<ParameterRefEntry parameterRef="COMMS_power_draw"/>
					<ParameterRefEntry parameterRef="COMMS_mode"/>
					<ParameterRefEntry parameterRef="COMMS_tm_bitrate"/>
					<ParameterRefEntry parameterRef="COMMS_tc_bitrate"/>
					<ParameterRefEntry parameterRef="PAYLOAD_state"/>
					<ParameterRefEntry parameterRef="PAYLOAD_temperature"/>
					<ParameterRefEntry parameterRef="PAYLOAD_heater_setpoint"/>
					<ParameterRefEntry parameterRef="PAYLOAD_power_draw"/>
					<ParameterRefEntry parameterRef="PAYLOAD_status"/>
					<ParameterRefEntry parameterRef="DATASTORE_state"/>
					<ParameterRefEntry parameterRef="DATASTORE_temperature"/>
					<ParameterRefEntry parameterRef="DATASTORE_heater_setpoint"/>
