            <Parameter name="ADCS_quaternion_q2" parameterTypeRef="float"/>
            <Parameter name="ADCS_quaternion_q3" parameterTypeRef="float"/>
            <Parameter name="ADCS_quaternion_q4" parameterTypeRef="float"/>
            <Parameter name="ADCS_angular_velocity_x" parameterTypeRef="float_deg_s"/>	
            <Parameter name="ADCS_angular_velocity_y" parameterTypeRef="float_deg_s"/>	
            <Parameter name="ADCS_angular_velocity_z" parameterTypeRef="float_deg_s"/>	
			<Parameter name="ADCS_earth_latitude" parameterTypeRef="float_deg"/>
			<Parameter name="ADCS_earth_longitude" parameterTypeRef="float_deg"/>	
			<Parameter name="ADCS_earth_altitude" parameterTypeRef="float_km"/>	
			<Parameter name="ADCS_eclipse" parameterTypeRef="Eclipse_Type"/>

            <!-- Communications Parameters -->
            <Parameter name="COMMS_state" parameterTypeRef="SubsystemState_Type"/>
            <Parameter name="COMMS_temperature" parameterTypeRef="int8_degC"/>
            <Parameter name="COMMS_heater_setpoint" parameterTypeRef="int8_degC"/>
            <Parameter name="COMMS_power_draw" parameterTypeRef="float_W"/>
