				<EnumerationList>
					<Enumeration label="IDLE" value="0"/>
					<Enumeration label="CAPTURING" value="1"/>
				</EnumerationList>
			</EnumeratedParameterType>	

			<!-- Datastore Mode Type -->	
			<EnumeratedParameterType name="DatastoreMode_Type">
				<IntegerDataEncoding sizeInBits="8"/>
				<EnumerationList>
					<Enumeration label="IDLE" value="0"/>
					<Enumeration label="TRANSFERING" value="1"/>
				</EnumerationList>
			</EnumeratedParameterType>

			<!-- Generic State -->	
			<EnumeratedParameterType name="GenericState_Type">
				<IntegerDataEncoding sizeInBits="8"/>
				<EnumerationList>
					<Enumeration label="OFF" value="0"/>
					<Enumeration label="ON" value="1"/>
				</EnumerationList>
			</EnumeratedParameterType>

			<!-- Eclipse Type -->
			<EnumeratedParameterType name="Eclipse_Type">
				<IntegerDataEncoding sizeInBits="8" />
				<EnumerationList>
					<Enumeration label="DAY" value="0"/>
					<Enumeration label="NIGHT" value="1"/>
