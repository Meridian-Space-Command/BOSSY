            <Parameter name="POWER_temperature" parameterTypeRef="int8_degC"/>
            <Parameter name="POWER_heater_setpoint" parameterTypeRef="int8_degC"/>
            <Parameter name="POWER_power_draw" parameterTypeRef="float_W"/>
            <Parameter name="POWER_battery_voltage" parameterTypeRef="float_V"/>
            <Parameter name="POWER_battery_current" parameterTypeRef="float_A"/>
            <Parameter name="POWER_battery_charge" parameterTypeRef="float_percent"/>
			<Parameter name="POWER_total_power_balance" parameterTypeRef="PowerBalance_Type"/>
			<Parameter name="POWER_total_power_draw" parameterTypeRef="float_W"/>
			<Parameter name="POWER_total_power_generation" parameterTypeRef="float_W"/>
            <Parameter name="POWER_solar_panel_generation_pX" parameterTypeRef="float_W"/>
            <Parameter name="POWER_solar_panel_generation_nX" parameterTypeRef="float_W"/>
			<Parameter name="POWER_solar_panel_generation_pY" parameterTypeRef="float_W"/>
			<Parameter name="POWER_solar_panel_generation_nY" parameterTypeRef="float_W"/>
			
            <!-- ADCS Parameters -->
            <Parameter name="ADCS_state" parameterTypeRef="SubsystemState_Type"/>
            <Parameter name="ADCS_temperature" parameterTypeRef="int8_degC"/>
            <Parameter name="ADCS_heater_setpoint" parameterTypeRef="int8_degC"/>
