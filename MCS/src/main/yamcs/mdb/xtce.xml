			</EnumeratedParameterType>

			<!-- ADCS Mode -->	
			<EnumeratedParameterType name="ADCSMode_Type">
				<IntegerDataEncoding sizeInBits="8"/>
				<EnumerationList>
					<Enumeration label="OFF" value="0"/>
					<Enumeration label="LOCK" value="1"/>
					<Enumeration label="SUNPOINTING" value="2"/>
					<Enumeration label="NADIR" value="3"/>
