			</IntegerParameterType>
			<IntegerParameterType name="uint32" signed="false">
				<IntegerDataEncoding encoding="unsigned" sizeInBits="32" />
			</IntegerParameterType>
			<IntegerParameterType name="uint32_bits" signed="false">
				<UnitSet>
					<Unit>bits</Unit>
				</UnitSet>
				<IntegerDataEncoding encoding="unsigned" sizeInBits="32" />
			</IntegerParameterType>
			<IntegerParameterType name="uint32_bps" signed="false">
				<UnitSet>
					<Unit>bps</Unit>
				</UnitSet>
