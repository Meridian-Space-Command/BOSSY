			</BooleanParameterType>

			<!-- Spacecraft Telemetry Types -->

			<!-- Telemetry Mode Type -->	
			<EnumeratedParameterType name="TelemetryMode_Type">
				<IntegerDataEncoding sizeInBits="8"/>
